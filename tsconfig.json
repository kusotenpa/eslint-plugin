{
    "compilerOptions": {
      "target": "esnext",
      "module": "CommonJS",
      "lib": [
        "esnext",
        "dom"
      ],
      "allowJs": true,
      "skipLibCheck": true,
      "strict": true,
      "noImplicitAny": false,
      "forceConsistentCasingInFileNames": true,
      "noEmit": true,
      "esModuleInterop": true,
      "moduleResolution": "node",
      "resolveJsonModule": true,
    },
    "include": [
      "**/*.ts",
    ],
    "exclude": [
      "node_modules"
    ]
  }
